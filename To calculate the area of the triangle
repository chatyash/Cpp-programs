#include<iostream.h>
using namespace std;

int main()
{
    float b, h, area;
    
    cout<<"Enter Base length of Triangle: ";
    
    cin>>b;
    
    cout<<"Enter Height of Triangle: ";
    
    cin>>h;
    
    area = 0.5*b*h;
    
    cout<<"\nArea = "<<area;
    
    cout<<endl;
    
    return 0;
    
}
